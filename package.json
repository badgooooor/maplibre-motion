{
  "name": "maplibre-motion",
  "version": "0.0.1",
  "description": "Maplibre plugin for animated routes",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "pnpm clean:dist && pnpm -r build",
    "dev": "pnpm -r dev",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "test": "pnpm -r test",
    "test:run": "pnpm -r test:run",
    "test:coverage": "pnpm -r test:coverage",
    "test:ui": "pnpm -r test:ui",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "clean:dist": "pnpm -r clean",
    "type-check": "pnpm -r type-check"
  },
  "keywords": [],
  "author": "Yuttakhan Baingern (https://yuttakhanb.dev)",
  "license": "MIT",
  "devDependencies": {}
}
