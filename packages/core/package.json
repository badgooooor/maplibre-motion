{
  "name": "@maplibre-motion/core",
  "version": "0.0.1",
  "description": "Core library for Maplibre animated routes",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "maplibre",
    "map",
    "animation",
    "route",
    "geojson"
  ],
  "author": "Yuttakhan Baingern (https://yuttakhanb.dev)",
  "license": "MIT",
  "dependencies": {
    "@turf/along": "^7.2.0",
    "@turf/bearing": "^7.2.0",
    "@turf/destination": "^7.2.0",
    "@turf/distance": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "@turf/length": "^7.2.0",
    "@turf/line-arc": "^7.2.0",
    "@turf/line-distance": "^4.7.3",
    "maplibre-gl": "^5.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/geojson": "^7946.0.16",
    "@types/maplibre-gl": "^1.14.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "maplibre-gl": "^5.0.0"
  }
}
